    </step>
    <step>
      <name>MRN0037</name>
      <type>1</type>
      <frame>13</frame>
      <resolution>0</resolution>
      <offset>0</offset>
      <description>设置Cyclone与低压MCU连接</description>
    </step>
    <step>
      <name>MRN0039</name>
      <type>7</type>
      <frame>14</frame>
      <resolution>1.0</resolution>
      <offset>0</offset>
      <description>设置DUT下电</description>
    </step>
    <step>
      <name>MRN0041</name>
      <type>7</type>
      <frame>15</frame>
      <resolution>1.0</resolution>
      <offset>0</offset>
      <description>读取DUT温感的基准电压</description>
    </step>
    <step>
      <name>MRNB0001</name>
      <type>8</type>
      <frame>1</frame>
      <resolution>0</resolution>
      <offset>0</offset>
      <description>BootLoader下载</description>
    </step>
    <step>
      <name>MRNB0002</name>
      <type>8</type>
      <frame>1</frame>
      <resolution>0</resolution>
      <offset>0</offset>
      <description>主MCU程序下载</description>
    </step>
    <step>
      <name>MRNB0003</name>
      <type>8</type>
      <frame>2</frame>
      <resolution>0</resolution>
      <offset>0</offset>
      <description>MCU1软件下载</description>
    </step>
    <step>
      <name>MRNB0004</name>
