<UserControl x:Class="FirstFloor.ModernUI.Windows.Controls.Waiting.Waiting2"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             xmlns:local="clr-namespace:FirstFloor.ModernUI.Windows.Controls"
             d:DesignHeight="60" d:DesignWidth="60">
    <Grid>
        <local:SprocketControl  
                        HorizontalAlignment="Center"
                        x:Name="sprocketControl1"
                        VerticalAlignment="Center"
                        TickColor="{DynamicResource AccentColor}"
                        Width="80"
                        Height="60"
                        TickWidth="3"
                        TickCount="12"
                        StartAngle="-90"
                        IsIndeterminate="True"
                        Interval="60" 
                        LowestAlpha="50"
                        AlphaTicksPercentage="50"/>
    </Grid>
</UserControl>
